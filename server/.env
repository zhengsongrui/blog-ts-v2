# 环境变量示例 - 复制此文件为 .env 并填写实际值

# 应用配置
NODE_ENV=development
PORT=3000
APP_NAME=BlogV2 Backend

# 数据库配置 (MySQL)
DATABASE_URL="mysql://root:@localhost:3307/bolgV2"

# JWT 配置
JWT_SECRET=your_jwt_secret_key_change_this_to_a_strong_random_string
JWT_EXPIRE=7d

# CORS 配置
CORS_ORIGIN=*

# 日志配置
LOG_LEVEL=info

# 速率限制
RATE_LIMIT_WINDOW_MS=9000000
RATE_LIMIT_MAX_REQUESTS=10000

# 其他
API_PREFIX=/api/v1